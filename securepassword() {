function user() {
  var username, password;
function doLogin(usr, pwd){
username=usr;
password=pwd;
  console.log("Welcome! you are logged in " + username)
}
var publicAPI= {
login:doLogin
}
return publicAPI;
}
var graceIbrahim= user();
